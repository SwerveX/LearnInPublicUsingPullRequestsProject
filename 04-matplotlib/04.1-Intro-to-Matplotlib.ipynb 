        "\n",
        "plt.figure(figsize = (10, 5))\n",
        "\n",
        "plt.plot(X, y, label = \"Sen X\")\n",
        "plt.plot(X, z, label = \"Cos X\")\n",
        "\n",
        "plt.title(\"Line plot\")\n",
        "plt.legend()\n",
        "plt.show()"
